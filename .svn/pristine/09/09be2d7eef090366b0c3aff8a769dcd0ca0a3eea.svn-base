body {
	font-family: Arial;
	font-size: 12px !important;
	line-height: 14px;
}

html {
	color: #000;
	background: #FFF;
}

body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,blockquote
	{
	margin: 0;
	padding: 0;
}

table {
	font-size: 12px !important;
}

fieldset,img {
	border: 0;
}

address,caption,cite,code,dfn,em,strong,th,var {
	font-style: normal;
	font-weight: normal;
}

li {
	list-style: none;
}

caption,th {
	text-align: left;
}

h1,h2,h3,h4,h5,h6 {
	font-size: 100%;
	font-weight: normal;
}

q:before,q:after {
	content: '';
}

abbr,acronym {
	border: 0;
	font-variant: normal;
}

sup {
	vertical-align: text-top;
}

sub {
	vertical-align: text-bottom;
}

input,textarea,select {
	font-family: inherit;
	font-size: inherit;
	font-weight: inherit;
}

input,textarea,select {
	*font-size: 100%;
}

legend {
	color: #000;
}

#footer {
	padding-left: 10px;
	background: url(../resources/images/bg/bg-footer.png) repeat-x center
		center;
	height: 29px;
	line-height: 25px;
	position: fixed;
	text-align: center;
	bottom: 0;
	width: 100%;
	display: block;
	border-top: 1px solid #6f7074;
	border-bottom: 1px solid #6f7074;
}

#navigator {
	height: 100px;
	width: 100%;
}

.name-project {
	margin: 4px 0px 0px 0px;
	height: 21px;
	width: 100%;
	
}

.left-name-project {
	height: 21px;
	width: 100%;
}

.name-poject {
	height: 21px;
	line-height: 21px;
	font-size: 26px;
	color: #00918e;
	font-weight: bold;
	text-align: center;
	padding: 20px 0px 20px 0px;
	width: 100%;	
}

.sub-name-poject {
	height: 21px;
	line-height: 15px;
	font-size: 16px;
	color: #d5832f;
	font-weight: bold;
	text-align: center;
	width: 100%;
}

.title-page {
	margin: 10px 0px 0px 140px;
	font-weight: bold;
}

.parent-title-page {
	color: #717171
}

.sub-title-page {
	color: #fefefe
}

.icon-image {
	overflow: visible;
	position: absolute;
	top: 9px;
	left: 21px;
}

.tab-content {
	height: 24px;
	padding-left: 70px;
	background: url(../resources/images/bg/bg-tab-content.png) repeat-x left
		top;
}

.tab-item-active {
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	border-top: 1px solid #b1b4bc;
	border-left: 1px solid #c8cbd3;
	border-right: 1px solid #c8cbd3;
	background: url(../resources/images/bg/bg-tab-active.png) repeat-x left
		top;
	height: 21px;
	padding: 0px 9px;
	margin-top: 2px;
	margin-right: 10px;
	line-height: 21px;
	font-weight: bold;
	cursor: pointer;
}

.tab-item-normal {
	padding: 0px 10px;
	margin-top: 2px;
	margin-right: 10px;
	line-height: 21px;
	font-weight: bold;
	cursor: pointer;
}

.tab-item-normal:hover {
	background-color: #e4e9f0;
	cursor: pointer;
}

.menu {
	height: 60px;
	background-color: #f7f7f7;
	border-bottom: 1px solid #bebebe
}

.tab-group-top {
	height: 64px;
	padding: 0px 0px;
}

.tab-icon {
	padding: 12px 5px;
}

.tab-icon-bottom {
	color: #383a39;
	font-size: 11px;
	text-align: center;
}

.tab-group-separator {
	width: 1px;
	background: url(../resources/images/bg/tab-group-separator.png)
		no-repeat left top;
	height: 60px;
}

.tab-icon-active {
	border: solid 1px #d8c894;
	padding: 3px;
	background: url(../resources/images/bg/tab-icon-active.png) repeat-x
		left top;
}

.tab-icon {
	float: left;
	text-align: center;
	margin: 0 5px;
}

.tab-icon-active {
	float: left;
	text-align: center;
	margin: 8px 5px;
}

.tab-icon-top {
	padding: 2px 0px;
}

#content {
	padding: 1px 0px 35px 0px;
	/* 	background: url(../resources/images/bg/bg-top-content.png) repeat-x left
		top; */
	overflow: hidden;
}

fieldset {
	display: block;
	padding: 5px;
	-webkit-margin-start: 2px;
	-webkit-margin-end: 2px;
	-webkit-padding-before: 0.35em;
	-webkit-padding-start: 0.75em;
	-webkit-padding-end: 0.75em;
	-webkit-padding-after: 0.625em;
	border: 2px groove threedface;
}

legend {
	display: block;
	-webkit-padding-start: 2px;
	-webkit-padding-end: 2px;
	border: none;
}

/*  Css for table list */
.tbl-list {
	width: 100%;
	/* border-top: 1px solid #6f7074; */
}

.table-header {
	background: url(../resources/images/bg/bg-td.png) repeat-x left top;
}

.table-header td {
	border-right: 1px solid #6f7074;
	border-bottom: 1px solid #6f7074;
	text-align: center;;
}

.table-header th {
	border-right: 1px solid #6f7074;
	border-bottom: 1px solid #6f7074;
	text-align: center;;
}

.tbl-list td {
	padding: 5px;
	cursor: pointer;
}

.tbl-list th {
	padding: 5px;
}

.table-filter td {
	border-right: 1px solid #6f7074;
	border-bottom: 1px solid #6f7074;
	padding: 1px !important
}

td.first {
	border-right: 1px solid #6f7074;
	border-bottom: 1px solid #6f7074;
	text-align: center;
	background: url(../resources/images/bg/bg-td.png) repeat-x left top;
}

.table-body td,.table-body-nodata td {
	border-right: 1px solid #e6e7e8;
	border-bottom: 1px solid #e6e7e8;
}

.table-body:hover {
	background-color: #EEEEEE;
	color: #000000;
}

.table-body-active {
	background-color: #D9ECF7;
	color: #000000;
}

.title-area {
	height: 19px;
	border: 1px solid #6f7074;
	border-right: none;
	background: url(../resources/images/bg/bg-title-area.png) repeat-x left
		top;
	text-align: center;
	line-height: 18px;
	color: #414141;
	font-weight: bold
}

.info-private-content {
	background-color: #f0f1f2;
	padding: 10px;
	border-bottom: 1px solid #6f7074;
	border-left: 1px solid #6f7074;
}

table .table-text {
	width: 98%;
}

.idleField {
	border: 2px solid #DFDFDF !important;
	/*color: #6F6F6F;*/;
	/*padding: 4px 3px;*/
}

.focusField {
	border: solid 2px #73A6FF !important;
	background: #EFF5FF;
	color: #000;
}
/*.focusField option{padding: 2px;}*/
.listobject option {
	padding: 3px;
}

.listgroup .idleField option {
	padding: 3px;
}